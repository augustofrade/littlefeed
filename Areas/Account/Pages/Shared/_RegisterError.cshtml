@model List<string>

<div>
    <div class="alert alert-danger mb-5" role="alert">
        Error Creating account.
        @if(Model.Count > 0) {
            <ul>
            @foreach (var error in Model)
            {
                <li>@error</li>
            }
            </ul>
        }
    </div>
    
    <div class="text-center">
        <a asp-page="@Url.Page("/")" class="btn btn-primary">Go back</a>
    </div>
</div>
